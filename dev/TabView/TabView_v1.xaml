<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:local="using:Microsoft.UI.Xaml.Controls"
    xmlns:primitives="using:Microsoft.UI.Xaml.Controls.Primitives">

    <Style TargetType="local:TabView" BasedOn="{StaticResource DefaultTabViewStyle}" />

    <Style TargetType="primitives:TabViewListView" BasedOn="{StaticResource DefaultTabViewListViewStyle}" />

    <Style TargetType="local:TabViewItem" BasedOn="{StaticResource DefaultTabViewItemStyle}" />

</ResourceDictionary>
