                            <RepeatButton x:Name="VerticalLargeDecrease" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Height="0" IsTabStop="False" Interval="50" Grid.Row="1" AllowFocusOnInteraction="False" Template="{StaticResource RepeatButtonTemplate}" />
                            <Thumb x:Name="VerticalThumb" Grid.Row="2" Background="{ThemeResource ScrollBarThumbBackground}" Template="{StaticResource VerticalThumbTemplate}" Width="12" MinHeight="12" AutomationProperties.AccessibilityView="Raw" />
