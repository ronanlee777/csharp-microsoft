<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:primitives="using:Microsoft.UI.Xaml.Controls.Primitives"
    xmlns:local="using:Microsoft.UI.Xaml.Controls">

    <Style TargetType="local:NavigationView" BasedOn="{StaticResource DefaultNavigationViewStyle}"/>

    <Style TargetType="local:NavigationViewItem" BasedOn="{StaticResource DefaultNavigationViewItemStyle}"/>

    <Style TargetType="primitives:NavigationViewItemPresenter" BasedOn="{StaticResource DefaultNavigationViewItemPresenterStyle}"/>

    <Style TargetType="local:NavigationViewItemHeader" BasedOn="{StaticResource DefaultNavigationViewItemHeaderStyle}"/>

    <Style TargetType="local:NavigationViewItemSeparator" BasedOn="{StaticResource DefaultNavigationViewItemSeparatorStyle}"/>

</ResourceDictionary>
